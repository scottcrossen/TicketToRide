<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.ajols.ticket_to_ride_gui_setup.JoinGame">

    <Spinner
        android:id="@+id/spinner2"
        android:layout_width="284dp"
        android:layout_height="46dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="91dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:entries="@array/media_names"
        app:layout_constraintHorizontal_bias="0.666" />

    <Button
        android:id="@+id/button"
        android:layout_width="143dp"
        android:layout_height="48dp"
        android:text="Start Game"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="47dp"
        app:layout_constraintBottom_toBottomOf="@+id/button2"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/button2"
        android:layout_marginLeft="47dp"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Quit"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="55dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="54dp"
        android:layout_marginRight="55dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="19dp"
        android:text="Color:"
        app:layout_constraintRight_toLeftOf="@+id/spinner2"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="9dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/spinner2"
        tools:layout_constraintBaseline_creator="1"
        android:layout_marginRight="9dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Player 1"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="16dp"
        android:layout_marginTop="23dp"
        app:layout_constraintTop_toBottomOf="@+id/spinner2"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Player 2"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/textView13"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Player 3"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="16dp"
        android:layout_marginTop="29dp"
        app:layout_constraintTop_toBottomOf="@+id/textView14"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Player 4"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="16dp"
        android:layout_marginTop="28dp"
        app:layout_constraintTop_toBottomOf="@+id/textView15"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Player 5"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="16dp"
        android:layout_marginTop="26dp"
        app:layout_constraintTop_toBottomOf="@+id/textView16"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="129dp"
        android:layout_height="0dp"
        android:text="Waiting on Player 1"
        android:layout_marginStart="14dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView13"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/textView13"
        android:layout_marginLeft="180dp"
        tools:layout_editor_absoluteY="180dp" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="129dp"
        android:layout_height="19dp"
        android:text="Waiting on Player 2"
        android:layout_marginStart="14dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView14"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/textView14"
        android:layout_marginLeft="14dp"
        tools:layout_editor_absoluteY="199dp" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="130dp"
        android:layout_height="20dp"
        android:text="Waiting on Player 3"
        android:layout_marginStart="14dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView15"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/textView15"
        android:layout_marginLeft="14dp"
        tools:layout_editor_absoluteY="243dp" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="130dp"
        android:layout_height="19dp"
        android:text="Waiting on Player 4"
        android:layout_marginStart="14dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView16"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/textView16"
        android:layout_marginLeft="14dp"
        tools:layout_editor_absoluteY="289dp" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="130dp"
        android:layout_height="21dp"
        android:text="Waiting on Player 5"
        android:layout_marginStart="14dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView17"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/textView17"
        android:layout_marginLeft="14dp"
        tools:layout_editor_absoluteY="329dp" />

</android.support.constraint.ConstraintLayout>
